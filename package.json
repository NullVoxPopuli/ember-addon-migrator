{
  "name": "ember-addon-migrator-root",
  "private": true,
  "license": "MIT",
  "author": "NullVoxPopuli",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/ember-addon-migrator",
    "type": "https",
    "directory": "./"
  },
  "scripts": {
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "format": "pnpm --filter '*' format",
    "format:check": "pnpm --filter '*' format:check"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "ember-cli-htmlbars"
      ],
      "ignore-notes": {
        "ember-cli-htmlbars": "required by ember-template-imports, via eslint-plugin-ember, via @nvp/eslint-configs"
      }
    }
  },
  "volta": {
    "node": "16.14.0"
  }
}